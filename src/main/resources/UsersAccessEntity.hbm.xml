<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.UsersAccessEntity" table="users_access" schema="app1_db">
        <id name="accessId" column="access_ID"/>
        <property name="accessLogin" column="access_login"/>
        <property name="accessPassword" column="access_password"/>
        <set name="usersByAccessId" inverse="true">
            <key>
                <column name="user_login"/>
            </key>
            <one-to-many not-found="ignore" class="entity.UsersEntity"/>
        </set>
    </class>
</hibernate-mapping>