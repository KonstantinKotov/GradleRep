<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>PVSite</display-name>




    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>MethodServlet</servlet-name>
        <servlet-class>TestServlet.MethodServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MethodServlet</servlet-name>
        <url-pattern>/method</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>signInServlet</servlet-name>
        <servlet-class>TestServlet.SignInServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>signInServlet</servlet-name>
        <url-pattern>/signIn</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>fServlet</servlet-name>
        <servlet-class>TestServlet.FServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>fServlet</servlet-name>
        <url-pattern>/f</url-pattern>
    </servlet-mapping>


</web-app>